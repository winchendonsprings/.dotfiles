! XTerm settings --------------------------
!XTerm*faceSize:	 10
!Xterm*faceName:	 Ubuntu\ Mono:pixelsize=10 
!------------------------------------------

! URXVT - Settings ------------------------
!load scripts
!URxvt.perl-lib: /home/ry/.urxvt/urxvt-perls/
!URxvt*termName: rxvt-256color
URxvt*saveline: 15000
URxvt*termName: urxvt
URxvt*iso14755: false
URxvt*iconFile: /usr/share/icons/Faience/apps/scalable/utilities-terminal.svg
!------------------------------------------

!keyboard select --------------------------
!is the ... needed?
!URxvt.perl-ext-common: ...,clipboard,default,keyboard-select,matcher,url-select
!URxvt.perl-ext-common: default,clipboard,keyboard-select,matcher,url-select
!URxvt.keysym.M-Escape: perl:keyboard-select:activate
!url-select
!URxvt.keysym.M-u: perl:url-select:select_next
!clipboard
!URxvt.keysym.M-c:   perl:clipboard:copy
!URxvt.keysym.M-v:   perl:clipboard:paste
!URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
!clickable urls
!URxvt.url-launcher: /usr/bin/firefox
!URxvt.matcher.button: 1
!yankable urls
!URxvt.perl-ext:      default,url-select
!URxvt.keysym.M-u:    perl:url-select:select_next
!URxvt.url-select.launcher:   /usr/bin/firefox -new-tab
!URxvt.url-select.underline: true
!Next three lines make ocopy and paste with keyboard doable
!URxvt.keysym.Shift-Control-V: perl:clipboard:paste
!------------------------------------------

! appearance ------------------------------
URxvt.geometry:			 92x56
URxvt.letterSpace:		 -1
URxvt.lineSpace:		 0
URxvt*depth:			 32
URxvt*scrollBar:		 false
URxvt*scrollBar_right:	 false
URxvt*internalBorder:	 5
URxvt*externalBorder:	 0
URxvt*borderLess:		 false
!URxvt.underlineColor:	 Yellow
!URxvt.highlightColor:	 Orange
!URxvt.throughColor:	 Blue
!URxvt*cursorColor2:	 Black
!------------------------------------------

